package com.stackroute.exercise3;

public class TryCatch {
	TryCatch(String msg) 
	{ 
	      
	} 
	
	
	public static void main(String[] args)throws Exception { 
		try { 
				throw new Exception();  
		     }catch(Exception e) { 
		    	 System.out.println(e.getMessage());
			} 
			finally 
			{ 
				System.out.println("iam here"); 
				} 
} 
}
