	private static char[] splitString(String input) {
		char[] result=null;
		for(int i=0;i<input.length();i++) {
			char x=input.charAt(0);
			if(x==(input.charAt(i))){
				result[i]=x;
				break;
			}else {
				char y=input.charAt(i);
				result[i]=result[i]=x;
				break;
			}
		}
		
		return result;
	}
	
	

}
